//WARNING Make sure for this project that in option.ini, needSemicolons is off!

using Console;
using Maths;

//this example shows that decimal numbers can be used too in wait()
var e = 0;
while (e == "0")
{
    Console.print("dance");
    wait(0.01);
    Console.print("till");
    wait(0.01);
    Console.print("your");
    wait(0.01);
    Console.print("dead");
    wait(0.01);
    Console.print("======");
    wait(0.2);
}